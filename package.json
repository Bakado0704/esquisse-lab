{
  "name": "esquisse-lab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:eslint": "eslint --ext .ts,.tsx --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-unknown src",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "format:css": "prettier-stylelint --write \"src/**/*.scss\"",
    "format": "npm run lint:eslint -- --fix && prettier --write --list-different src",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "date-fns": "^3.2.0",
    "eslint": "8.39.0",
    "next": "13.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^5",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "destyle.css": "^4.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.1",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.7.1",
    "prettier-stylelint": "^0.4.2",
    "sass": "^1.72.0",
    "stylelint": "^16.6.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-scss": "^6.3.1"
  }
}
